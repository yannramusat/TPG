<Mappings>

<!-- 
inproceedings(bookid, title, bktitle, year, month, pages, vol, num, loc, class, note, annote) 
& inprocpublished(inprocId, authId) 
& author(authId, authName) 
-> 
article(SK1(bookid), title, vol, num, pages, month, year, SK2(bookid), note, SK3(bookid), SK4(bookid), SK5(bookid), SK6(bookid), SK7(bookid), SK8(bookid), SK9(bookid)) 
& articleauthor(SK1(bookid), authId)
& author(authId, authName) 
-->

	<Mapping id="M1">
  		<Uses>
			<Correspondence ref="c1" />
			<Correspondence ref="c2" />
			<Correspondence ref="c11" />
			<Correspondence ref="c12" />
			<Correspondence ref="c13" />
			<Correspondence ref="c14" />
			<Correspondence ref="c15" />
			<Correspondence ref="c16" />
		</Uses>
		<Foreach>
			<Atom tableref="inproceedings">
			  <Var>inprocid</Var>
			  <Var>title</Var>
			  <Var>bktitle</Var>
			  <Var>year</Var>
			  <Var>month</Var>
			  <Var>pages</Var>
			  <Var>vol</Var>
			  <Var>num</Var>
			  <Var>loc</Var>
			  <Var>class</Var>
			  <Var>note</Var>
			  <Var>annote</Var>
			</Atom>
			<Atom tableref="inprocpublished">
			  <Var>inprocId</Var>
			  <Var>authId</Var>
			</Atom>
			<Atom tableref="author">
			  <Var>authId</Var>
			  <Var>authName</Var>
			</Atom>
		</Foreach>
